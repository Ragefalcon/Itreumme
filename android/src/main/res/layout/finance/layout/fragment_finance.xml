<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".ui.finance.FinanceMainScreen"
>

    <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_finance_list"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="4dp"
            app:layout_constraintBottom_toTopOf="@+id/tv_fin_period"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:listitem="@layout/item_rasxod" android:layout_marginLeft="12dp" android:layout_marginRight="12dp"/>
    <TextView
            android:text="0.0"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/tv_fin_period"
            app:layout_constraintBottom_toTopOf="@+id/sr_schet"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:gravity="right"
            android:textColor="@color/colorRasxodItem"
            android:textSize="24sp"
            android:shadowColor="@android:color/background_dark"
            android:shadowDx="2"
            android:shadowDy="2"
            android:shadowRadius="3"
            android:paddingRight="16dp"
            android:paddingLeft="16dp"
            android:layout_marginBottom="4dp"
            android:background="@drawable/bg_analiz_main"
    />
    <Spinner
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:id="@+id/sr_schet"
            app:layout_constraintBottom_toTopOf="@+id/sr_filter"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:visibility="invisible"
            tools:visibility="visible"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            tools:listitem="@layout/simple_list_item_white"
    />
    <Spinner
            android:layout_width="0dp"
            android:layout_height="1dp"
            tools:layout_height="40dp"
            android:id="@+id/sr_filter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:visibility="invisible"
            tools:visibility="visible"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            tools:listitem="@layout/simple_list_item_white"
    />
</androidx.constraintlayout.widget.ConstraintLayout>
